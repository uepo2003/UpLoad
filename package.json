  "scripts": {
    "test": "jest -c ./e2e/jest.config.js",
    "test:image_file_input": "npm run test -- user/edit.ts -t 'User edit page sign in with correct user display image file input'",
    "test:image_file_upload": "npm run test -- usecases/update_user.ts -t 'Update user submit success changes user image when image file is selected'",
    "test:not_image_attach": "npm run test -- usecases/update_user.ts -t 'Update user submit success does not change user image when image file is empty'",
    "test:typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "watch": "nodemon -r tsconfig-paths/register --files --watch ./src ./bin/run_server.ts",
    "lint:check": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "format:check": "prettier --check '**/*.{js,ts}'",
    "format:fix": "prettier --write '**/*.{js,ts}'",
    "db:reset": "ts-node -r tsconfig-paths/register --files ./bin/reset_db.ts",
    "start": "ts-node -r tsconfig-paths/register --files ./bin/run_server.ts"
  },
